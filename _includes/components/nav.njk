  <div class="col-12 col-md-9 px-0 mt-4 d-flex justify-content-center">
    <nav>
      <ul class="nav">
        {% for nav in collections.nav %}
        <li class="nav-item {% if nav.url == page.url %}active{% endif %} ">
          <a href="{{ nav.url | url }}"
            class="nav-link text-light mx-1 {% if loop.first %}rounded-left{% endif %} {% if loop.last %}rounded-right{% endif %}">{{ nav.data.navtitle or nav.data.title }}</a>
        </li>
        {% endfor %}

      </ul>
    </nav>

  </div>

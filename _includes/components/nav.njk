{% if navigation.items %}

  <nav>
    <button id="nav_toggler" type="button" aria-expanded="false">Menu</button>

    <ul class="nav_list">
      {% for nav in navigation.items %}
        <a href="{{ nav.url | url }}" class="nav_item">
          <li>
            {{ nav.text }}
          </li>
        </a>
      {% endfor %}

    </ul>
  </nav>

  {% endif%}
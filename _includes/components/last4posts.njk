{% set year = "" %}
{% for i in range(0, 4) %}
	<section class="section">
		<p class="post_date">{{ postslist[i].date | readableDate }}</p>
		<h2 class="post_title">
			<a href="{{ postslist[i].url | url }}" title="{{ postslist[i].data.title }}">{{ postslist[i].data.title }}</a>
		</h2>
	</section>
{% endfor %}